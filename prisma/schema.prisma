generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Provider {
  id             String   @id @default(uuid())
  score          Int      @default(0)
  calls          Int      @default(0)
  injectionToken String
  instance       String   @unique
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
